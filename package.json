{
  "name": "centralized-alerting",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy --all --require-approval never",
    "diff": "cdk diff --all",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@eslint/js": "9.27.0",
    "@types/aws-lambda": "8.10.147",
    "@types/node": "22.15.21",
    "aws-cdk": "2.1033.0",
    "esbuild": "0.24.1",
    "eslint": "9.27.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.4.0",
    "prettier": "3.4.2",
    "tsx": "4.21.0",
    "typescript": "5.6.3",
    "typescript-eslint": "8.32.1"
  },
  "dependencies": {
    "aws-cdk-lib": "2.232.1",
    "constructs": "10.4.2"
  }
}
